<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>FirstServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>WelcomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Quizquestions</servlet-name>
    <servlet-class>Qservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Resultname</servlet-name>
    <servlet-class>Res</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Display</servlet-name>
    <servlet-class>DisplayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pdffile</servlet-name>
    <servlet-class>Pdf</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>emailfile</servlet-name>
    <servlet-class>Email</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/servlet2</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Quizquestions</servlet-name>
    <url-pattern>/questions</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Resultname</servlet-name>
    <url-pattern>/servlet3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Display</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pdffile</servlet-name>
    <url-pattern>/pdf</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>emailfile</servlet-name>
    <url-pattern>/mail</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <display-name>MySQL DataSource Example</display-name>
  <resource-ref>
	<description>MySQL Datasource example</description>
	<res-ref-name>jdbc/ebookshop</res-ref-name>
	<res-type>javax.sql.DataSource</res-type>
	<res-auth>Container</res-auth>
  </resource-ref> 
</web-app>
